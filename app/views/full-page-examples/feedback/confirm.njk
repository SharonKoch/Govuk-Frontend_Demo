{% extends "_generic.njk" %}

{% from "panel/macro.njk" import moalandPanel %}

{% set pageTitle = "Thank you for your feedback" %}
{% block pageTitle %}{{ pageTitle }} - GOV.MOA{% endblock %}

{% block header %}
  {% include "../../partials/banner.njk" %}
  {{ moalandHeader({
    productName: "Verify"
  }) }}
{% endblock %}

{% block content %}
  <div class="moaland-grid-row">
    <div class="moaland-grid-column-two-thirds">
      {{ moalandPanel({
        titleText: pageTitle
      }) }}
    </div>
  </div>
{% endblock %}
