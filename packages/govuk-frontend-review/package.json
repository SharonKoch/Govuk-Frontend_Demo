{
  "private": true,
  "name": "@govuk-frontend/review",
  "description": "GOV.UK Frontend review app",
  "main": "src/start.mjs",
  "engines": {
    "node": "^18.12.0",
    "npm": "^8.1.0 || ^9.1.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "gulp dev --color",
    "prebuild": "npm run clean",
    "build": "gulp build --color",
    "build:sassdoc": "sassdoc --config sassdoc.config.yaml ../govuk-frontend/src/govuk",
    "build:types": "tsc --build tsconfig.build.json",
    "build:jsdoc": "typedoc",
    "postbuild": "npm run build:jsdoc && npm run build:sassdoc",
    "clean": "del-cli dist",
    "proxy": "browser-sync start --config browsersync.config.js",
    "serve": "nodemon",
    "start": "node src/start.mjs",
    "test:screenshots": "gulp screenshots --color"
  },
  "dependencies": {
    "@govuk-frontend/config": "*",
    "@govuk-frontend/lib": "*",
    "@govuk-frontend/stats": "*",
    "@prettier/sync": "^0.3.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "front-matter": "^4.0.2",
    "govuk-frontend": "*",
    "highlight.js": "^11.8.0",
    "iframe-resizer": "^4.3.7",
    "js-beautify": "^1.14.9",
    "marked": "^9.0.3",
    "marked-linkify-it": "^3.1.4",
    "marked-smartypants": "^1.1.3",
    "outdent": "^0.8.0",
    "shuffle-seed": "^1.1.6",
    "slug": "^8.2.3"
  },
  "devDependencies": {
    "@govuk-frontend/helpers": "*",
    "@govuk-frontend/tasks": "*",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "cssnano": "^6.0.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "nodemon": "^3.0.1",
    "postcss-pseudo-classes": "^0.2.1",
    "puppeteer": "^21.3.6",
    "rollup": "^3.29.4",
    "sassdoc": "^2.7.4",
    "slash": "^5.1.0",
    "supertest": "^6.3.3",
    "typedoc": "^0.25.1"
  },
  "optionalDependencies": {
    "nunjucks": "^3.2.4"
  }
}
