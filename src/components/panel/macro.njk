{% macro govukPanel(
  params,
  text,
  safe,
  title,
  classes,
  attributes
) %}
  {% if params.html and not hideDeprecated %}
    <strong class="deprecated">params.html is deprecated in govukPanel</strong>
  {% endif %}
  {% if params.title.html and not hideDeprecated %}
    <strong class="deprecated">params.title.html is deprecated in govukPanel</strong>
  {% endif %}
  {% set text = text if text else params.text %}
  {% if safe === undefined %}{% set safe = params.safe %}{% endif %}
  {% set title = title if title else params.title %}
  {% set classes = classes if classes else params.classes %}
  {% set attributes = attributes if attributes else params.attributes %}
  {% set params = {
    text: text,
    safe: safe,
    title: title,
    classes: classes,
    attributes: attributes
  } %}
  {%- include "./template.njk" -%}
{% endmacro %}
